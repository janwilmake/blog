{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",

  "name": "blog-worker",
  "main": "worker.ts",
  "compatibility_date": "2025-12-01",
  "assets": {
    "directory": "./",
    "binding": "ASSETS"
  },
  "vars": {
    "NAME": "Jan Wilmake's Blog",
    "GITHUB": "https://github.com/janwilmake/blog",
    "X": "https://x.com/janwilmake"
  },
  "routes": [{ "custom_domain": true, "pattern": "blog.janwilmake.com" }]
}
